<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="DemoSuite" parallel="methods" thread-count="3">
  <test name="AllTestsRun">
    <groups>
      <run>
        <include name="sanity"/>
        <include name="regression"/>
      </run>
    </groups>
    <classes>
      <class name="testing.AllTests"/>
    </classes>
  </test>
</suite>
